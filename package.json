{
    "name": "commonplace-ccmcg",
    "description": "A beautiful, free, standards-compliant theme, specifically designed for content marketing.",
    "version": "1.4.15",
    "engines": {
        "ghost": ">=5.0.0"
    },
    "license": "MIT",
    "author": {
        "email": "cedric@commoncog.com"
    },
    "keywords": [
      "sass",
      "ghost-theme"
    ],
    "gpm": {
        "type": "theme",
        "categories": [
            "Business Blogs",
            "Grid Based"
        ]
    },
    "card_assets": {
      "exclude": ["nft"]
    },
    "config": {
        "posts_per_page": 6
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch": "NODE_ENV=development webpack --watch",
        "build": "NODE_ENV=production webpack"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.6",
        "copy-webpack-plugin": "^4.2.1",
        "css-loader": "^0.28.7",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.5",
        "node-sass": "^4.12.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "postcss-loader": "^2.0.8",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "url-loader": "^0.6.2",
        "ajv": "^6.4.0",
        "webpack": "^3.6.0"
    },
    "dependencies": {
        "foundation-sites": "~6.4.3",
        "gscan": "^1.4.2"
    }
}
